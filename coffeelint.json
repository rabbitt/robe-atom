{
  "cyclomatic_complexity": {
    "value": 10,
    "level": "warn"
  },
  "eol_last": {
    "level": "error"
  },
  "line_endings": {
    "level": "error",
    "value": "unix"
  },
  "max_line_length": {
    "value": 120,
    "level": "error",
    "limitComments": true
  },
  "missing_fat_arrows": {
    "level": "ignore",
    "is_strict": false
  },
  "no_implicit_parens": {
    "strict": true,
    "level": "ignore"
  },
  "no_stand_alone_at": {
    "level": "error"
  },
  "no_this": {
    "level": "warn"
  },
  "no_unnecessary_double_quotes": {
    "level": "warn"
  },
  "prefer_english_operator": {
    "level": "warn"
  }
}
